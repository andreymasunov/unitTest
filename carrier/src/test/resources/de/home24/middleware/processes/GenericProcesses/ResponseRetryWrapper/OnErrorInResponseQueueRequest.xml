			<ns4:sourceSystemName>Middleware</ns4:sourceSystemName>
			<ns4:environment></ns4:environment>
			<ns4:transactionId>123456</ns4:transactionId>
			<ns4:flowId></ns4:flowId>
			<ns4:ecid></ns4:ecid>
			<ns4:activityId>P101-PAY-ERR</ns4:activityId>
